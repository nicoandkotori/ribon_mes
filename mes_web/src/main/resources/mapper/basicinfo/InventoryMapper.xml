    t.cInvCCode,t.cInvCCode+' '+ t.cInvCName cInvCName,t1.cinvccode parentCode,t1.cinvcname parentName
    from InventoryClass t left join InventoryClass t1 on case when len(t.cInvCCode)>2 then left(t.cInvCCode,len(t.cInvCCode)-2)  else ''end=t1.cinvccode
    where t.binvcend=1
  </select>
