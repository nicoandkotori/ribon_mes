  <select id="getList" resultMap="BaseResultMap" parameterType="com.web.basicinfo.entity.Inventory" >
    select
    a.cInvCode,a.cInvName,a.cInvAddCode,a.cInvStd,b.cComUnitName,cInvDefine6,a.cInvDefine2,a.cInvDefine1,a.cInvDefine3,c.cComUnitName ccomunitnameassist,c.iChangRate ichangrate
    from Inventory a
    left join ComputationUnit b on a.cComUnitCode = b.cComUnitCode
    left join ComputationUnit c on a.cSTComUnitCode = c.cComUnitCode
    where 1=1
    <if test="main.cinvcode != null and main.cinvcode != ''">
      AND a.cinvcode like '%' + #{main.cinvcode} + '%'
    </if>
    <if test="main.cinvname != null and main.cinvname != ''">
      AND a.cInvName like '%' + #{main.cinvname} + '%'
    </if>
    <if test="main.cinvaddcode != null and main.cinvaddcode != ''">
      AND a.cInvAddCode like '%' + #{main.cinvaddcode} + '%'
    </if>
    <if test="main.cinvstd != null and main.cinvstd != ''">
      AND a.cInvStd like '%' + #{main.cinvstd} + '%'
    </if>
    <if test="main.cinvccode != null and main.cinvccode != ''">
      AND a.cInvCCode like '%' + #{main.cinvccode} + '%'
    </if>
    <if test="main.cinvdefine6 != null and main.cinvdefine6 != ''">
      AND a.cInvDefine6 like '%' + #{main.cinvdefine6} + '%'
    </if>
    order by cInvCode
  </select>



  <select id="getInfoById" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select
    a.cInvCode,a.cInvName,a.cInvAddCode,a.cInvStd,a.cComUnitCode,b.cComUnitCode ccomunitcodeassist,b.cComUnitName,cInvDefine6,cInvCCode,a.cInvDefine1,cInvDefine2,cInvDefine3,c.cComUnitName ccomunitnameassist,c.iChangRate ichangrate,a.iInvNCost,a.bProxyForeign,a.bPurchase
    from Inventory a
    left join ComputationUnit b on a.cComUnitCode = b.cComUnitCode
    left join ComputationUnit c on a.cSTComUnitCode = c.cComUnitCode
    where cInvCode = #{cinvcode,jdbcType=VARCHAR}
  </select>

  <select id="getPrice" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select top 1 iInCost  iInvNCost from IA_Subsidiary
    where cInvCode = #{cinvcode,jdbcType=VARCHAR} and cBusType='普通采购'
    order by dVouDate desc
  </select>
  <select id="getPriceFirst" resultMap="BaseResultMap" parameterType="java.lang.String" >
    select top 1 iUnitPrice  iInvNCost
    from po_pomain t left join po_podetails t1 on t.poid=t1.poid
    where cInvCode = #{cinvcode,jdbcType=VARCHAR} and iUnitPrice!=0
    order by dpodate desc
  </select>


  <select id="selectParentMenuList" resultMap="BaseResultMap">
    select
      cInvCCode,cInvCCode+' '+ cInvCName cInvCName, case when len(cInvCCode)>2 then left(cInvCCode,len(cInvCCode)-2)  else '' end parentcinvccode
    from InventoryClass
    where case when len(cInvCCode)>2 then left(cInvCCode,len(cInvCCode)-2)  else '' end =''
  </select>

